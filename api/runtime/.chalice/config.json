{
  "version": "2.0",
  "app_name": "API",
  "stages": {
    "UserManagementBackend-Dev/Stateless/API": {
      "api_gateway_stage": "v1",
      "lambda_functions": {
        "api_handler": {
          "manage_iam_role": false,
          "iam_role_arn": "${Token[TOKEN.96]}",
          "environment_variables": {
            "TABLE_NAME": "${Token[TOKEN.87]}"
          },
          "lambda_memory_size": 128,
          "lambda_timeout": 10,
          "reserved_concurrency": 1,
          "xray": true
        }
      }
    },
    "UserManagementBackend-Pipeline/UserManagementBackend-Prod/Stateless/API": {
      "api_gateway_stage": "v1",
      "lambda_functions": {
        "api_handler": {
          "manage_iam_role": false,
          "iam_role_arn": "${Token[TOKEN.173]}",
          "environment_variables": {
            "TABLE_NAME": "${Token[TOKEN.164]}"
          },
          "lambda_memory_size": 128,
          "lambda_timeout": 10,
          "reserved_concurrency": 10,
          "xray": true
        }
      }
    }
  }
}